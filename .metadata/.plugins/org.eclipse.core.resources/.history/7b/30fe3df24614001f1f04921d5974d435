package demo;

public class B extends A{
	
	 Integer num = 20;

	    public void display() {

	        System.out.println("In Child class");

	    }
	    
	    public static void main(String[] args) {
	    	
	    	A a=new B();
	    	a.display();
	    
	    	
	    	B b=new B();
	    	b.display();
	    	
	    
	    	
	    	
	    	
	    	
	    	
			
		}

}
